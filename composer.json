{
    "name": "jinyerp/uikit",
    "description": "UI Kit package for Jiny ERP system",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Jiny ERP Team",
            "email": "support@jinyerp.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Uikit\\": "/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Uikit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Uikit\\JinyUikitServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "uikit",
        "components",
        "blade",
        "jiny",
        "erp"
    ],
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
